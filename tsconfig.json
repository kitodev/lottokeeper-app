{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "es2015", "es2017.object"],
      "jsx": "react-jsx",
      "module": "esnext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "isolatedModules": true,
      "strict": true
    },
    "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.js", "src/**/*.jsx"],
    "exclude": ["node_modules"]
  }
  